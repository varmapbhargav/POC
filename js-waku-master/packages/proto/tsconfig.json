{
  "extends": "../../tsconfig",
  "compilerOptions": {
    "outDir": "dist/",
    "rootDir": "src",
    "tsBuildInfoFile": "dist/.tsbuildinfo",
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": ["src/index.ts", "src/lib/**/*.proto"],
  "exclude": ["src/generated/**/*.ts"]
}